{
  "manifest_version": 3,
  "name": "Dashboard Rotator",
  "description": "This extension automatically switches between a list of dashboard URLs at a specified time interval",
  "version": "1.01",
  "permissions": [
      "storage",
      "tabs"
  ],
  "background": {
      "service_worker": "background.js"
  },
  "action": {
      "default_popup": "popup.html",
      "default_title": "Dashboard Rotator"
  },
  "icons": {
    "16": "icons/disabled_icon_16.png",
    "32": "icons/enabled_icon_32.png",
    "48": "icons/enabled_icon_48.png",
    "128": "icons/enabled_icon_128.png"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self';"
  }
}
